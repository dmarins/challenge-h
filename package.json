{
  "name": "presentation-ui",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "test:unit": "react-scripts test",
    "test:unit:debug": "node --inspect node_modules/.bin/jest --watch --runInBand",
    "test:e2e": "yarn cy:run",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "test:ci": "yarn test:unit --watchAll=false --coverage",
    "test:lintstaged": "yarn test:unit --watchAll=false --bail --findRelatedTests",
    "prepare": "husky install",
    "pre-commit-lint": "yarn lint-staged",
    "lint": "eslint src --max-warnings=0 --fix && prettier --write src",
    "generate": "yarn plop --plopfile ./.generators/presentation/plopfile.js"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "axios": "^0.21.1",
    "i18next": "^20.2.2",
    "i18next-browser-languagedetector": "^6.1.1",
    "normalize.css": "^8.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.8.15",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "react-toastify": "^7.0.4",
    "typescript": "^4.1.2"
  },
  "devDependencies": {
    "cypress": "^7.3.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.2",
    "eslint-plugin-prettier": "^3.4.0",
    "faker": "^5.5.3",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "plop": "^2.7.4",
    "prettier": "^2.3.0",
    "sort-package-json": "^1.50.0"
  }
}
